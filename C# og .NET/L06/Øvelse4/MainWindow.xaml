<Window x:Class="Øvelse4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Øvelse4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel Height="400" Width="759" HorizontalAlignment="Center">
            <Button Content="Left" Height="50" Width="50" Click="Button_Click"/>
            <Button Content="Top" Height="50" Width="50" DockPanel.Dock="Top" Click="Button_Click"/>
            <Button Content="Right" Height="50" Width="50" DockPanel.Dock="Right" Click="Button_Click"/>
            <Button Content="Buttom" Height="50" Width="50" DockPanel.Dock="Bottom" Click="Button_Click"/>
            <TextBox Height="200" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="200" HorizontalAlignment="Center" x:Name="textbox"/>

        </DockPanel>

    </Grid>
</Window>
